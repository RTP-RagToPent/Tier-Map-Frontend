# Supabase設定（認証とキャッシュ用）
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# サーバーサイド用（Geocoding API、Places API用）
GOOGLE_MAPS_API_KEY_SERVER=your_google_maps_api_key_here

# Google Maps API（スポット検索用）
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Backend API Base URL（Supabase Edge Functions）
NEXT_PUBLIC_API_BASE_URL=https://your-project.supabase.co/functions/v1
